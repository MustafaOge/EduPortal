@model EduPortal.MVC.Models.Subscriber.SubscriberModel


<h3>Subscriber Create</h3>

<div class="register-box" >
    <div class="lb-header">
        <a href="#" class="active" id="individual-box-link">@_localization.Getkey("IndividualSubscription").Value</a>
        <a href="#" id="corporate-box-link">@_localization.Getkey("CorporateSubscription").Value</a>
    </div>

    <div class="individual-find">
        @Html.Partial("Subscriber/CreateIndividual") 
    </div>
    <div class="corporate-find">
        @Html.Partial("Subscriber/CreateCorporate")
    </div> 
</div>


@if (TempData["Message"] != null)
{
    <h2>Unknown Error </h2>
    <p class="text-danger">@TempData["Message"]</p>
}


@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}

